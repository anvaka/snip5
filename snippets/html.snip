snippet html
	<!DOCTYPE html>
	<html lang="en">
	<head>
	  <title>${1}</title>
	</head>
	<body>
	  ${2}
	</body>
	</html>

snippet a
options word
	<a href="${1:url}">${2:name}</a>

snippet img
options word
	<img src="${1}">

snippet script
	<script src="${0}.js"></script>

snippet link
	<link rel="stylesheet" href="${0}.css">
